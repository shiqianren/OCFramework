platform:ios,'7.0'
target 'OCPRO' do
pod 'AFNetworking','~>3.1.0'
pod 'MJExtension'
pod 'IQKeyboardManager'
pod 'SDWebImage','~> 3.7.5'
pod 'Masonry'
# 键盘框架
pod 'IQKeyboardManager'
# 在屏幕中间显示 加载框 类似于安卓的toast效果
pod 'MBProgressHUD'
#HexColors is an extension for UIColor and NSColor to support for creating colors from a hex string like #FF0088 or 8844FF and back to a String. Completely rewritten in Swift 3!
pod 'HexColors', '~> 2.2.1'
#This is an extremely simple NSString category that creates an MD5 digest from a given NSString
pod 'MD5Digest', '~> 1.1.0'
#objective-C Base64 Additions for NSData and NSString
pod 'NSData+Base64', '~> 1.0.0'
# 网络或本地 多张图片浏览 控制器
#    pod 'MWPhotoBrowser'
# WebView的进度条
#    pod 'NJKWebViewProgress'
#SSKeychain is a simple wrapper for accessing accounts, getting passwords, setting passwords, and deleting passwords using the system Keychain on Mac OS X and iOS.
pod 'SSKeychain'
end


















